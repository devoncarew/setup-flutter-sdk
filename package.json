{
  "name": "setup-flutter-sdk",
  "version": "1.1.0",
  "description": "A GitHub action to install the Flutter SDK.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/index.js -o dist --minify",
    "lint": "eslint src/",
    "test": "jest"
  },
  "dependencies": {
    "@actions/cache": "^4.0.0",
    "@actions/core": "^1.10.1",
    "@actions/http-client": "^2.2.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/src/**/*.test.ts"]
  }
}
